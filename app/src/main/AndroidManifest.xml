<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="tshirt.extremedev.tapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light"
        tools:replace="android:icon"
        >
        <activity
            android:name="tshirt.extremedev.tapp.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tshirt.extremedev.tapp.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tshirt.extremedev.tapp.SignupActivity"
            android:label="Sign Up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tshirt.extremedev.tapp.SignIN"
            android:label="TApp"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="tshirt.extremedev.tapp.Testing"
            android:label="@string/title_activity_testing"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tshirt.extremedev.tapp.Grocery"
            android:label="Select Item"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tshirt.extremedev.tapp.Utensils"
            android:label="Previous Shopping Lists"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="tshirt.extremedev.tapp.Events"
            android:label="@string/title_activity_events"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="tshirt.extremedev.tapp.MoreDetails" />
        <activity
            android:name="tshirt.extremedev.tapp.Details"
            android:label="@string/title_activity_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="tshirt.extremedev.tapp.ImageEditor" />
        <activity android:name="tshirt.extremedev.tapp.Cart"

            ></activity>
    </application>

</manifest>